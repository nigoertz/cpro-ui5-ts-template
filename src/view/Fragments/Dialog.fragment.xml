<core:FragmentDefinition xmlns:m="sap.m" xmlns:core="sap.ui.core">
    <m:Dialog id="popupDialog" title="{settings>/title}" bindingContext="dialogContext">
        <m:HBox justifyContent="Center">
            <m:Button text="{settings>/emphasizedButtonText}" press="onPrimaryPress"   type="{settings>/typePrimary}"   class="sapUiSmallMargin"/>          
            <m:Button text="{settings>/rejectButtonText}"     press="onSecondaryPress" type="{settings>/typeSecondary}" class="sapUiSmallMargin"/>
        </m:HBox>
    </m:Dialog>
</core:FragmentDefinition>